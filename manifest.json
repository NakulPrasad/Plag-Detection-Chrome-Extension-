{
    "manifest_version": 3,
    "name": "Plag Detection",
    "version": "1.0",
    "description": "Description of my extension",
    "permissions": [
      "activeTab"
    ],
    "action": {
      "default_popup": "popup/popup.html"
    },
    "background": {
      "service_worker": "background/background.js"
    },
    "content_scripts": [
        {
          "matches": ["https://mentorpick.com/*"],
          "js": ["popup/popup.js"]
        }
     ]
  }
  